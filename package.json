{
  "name": "topoli",
  "version": "1.0.0",
  "description": "Visualizing Toronto's political data",
  "main": "server/index.js",
  "scripts": {
    "build": "npm run clean && node node_modules/.bin/webpack",
    "clean": "rimraf dist",
    "dev": "node node_modules/.bin/webpack-dev-server --content-base app --hot",
    "postinstall": "node node_modules/.bin/typings install",
    "server": "nodemon server --ignore src --ignore node_modules --ignore dist",
    "start": "npm run build && npm run server",
    "test": ""
  },
  "engines": {
    "node": "5.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abmohan/topoli"
  },
  "keywords": [
    "toronto",
    "political",
    "data",
    "map",
    "geo",
    "canada"
  ],
  "author": "Ashwin Balamohan <abmohan@gmail.com>",
  "contributors": [
    {
      "name": "Ashwin Balamohan",
      "email": "abmohan@gmail.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abmohan/topoli/issues"
  },
  "homepage": "https://github.com/abmohan/topoli",
  "dependencies": {
    "angular2": "^2.0.0-beta.17",
    "d3": "^3.5.16",
    "es6-shim": "^0.35.0",
    "express": "^4.13.4",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.6",
    "sequelize": "^3.22.0",
    "winston": "^2.2.0",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "html-webpack-plugin": "^2.16.0",
    "nodemon": "^1.9.2",
    "request-promise": "^3.0.0",
    "rimraf": "^2.5.2",
    "ts-loader": "^0.8.2",
    "tslint": "^3.8.1",
    "tslint-loader": "^2.1.4",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-livereload-plugin": "^0.8.1"
  }
}
